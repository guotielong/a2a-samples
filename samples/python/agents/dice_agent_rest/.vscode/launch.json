{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Dice REST Agent",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/__main__.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "cwd": "${workspaceFolder}",
      "args": [
        "--host",
        "localhost",
        "--port",
        "10101"
      ]
    }
  ]
}
