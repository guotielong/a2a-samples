{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Orchestrator Agent",
      "type": "python",
      "request": "launch",
      "module": "a2a_mcp.agents",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--agent-card", "agent_cards/orchestrator_agent.json",
        "--port", "10101"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}